{% extends "::base.html.twig" %}

{% block title %}Register to Client Service by REGG{% endblock %}



{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('assets/vendor/bootstrap/dist/css/bootstrap.min.css') }}">
{% endblock %}

{% block body %}
    <h1>Register to REGG System for administration Client</h1>

    <h2 class="text-center text-muted">Register</h2><hr>
    {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}

    <div class="col-md-6 col-md-offset-3">
        {{ form_widget(form) }}
        {{ form_errors(form) }}
    </div>

    {{ form_end(form) }}

{% endblock %}